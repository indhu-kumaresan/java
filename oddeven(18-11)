package Task;


public class odd {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        System.out.println("ODD NUMBERS\n");
        for(int i=0;i<=10;i++){
            if(i %2!=0){
                System.out.print( i + " ");
            }
        }
       System.out.println("\nEVEN NUMBERS\n");
        for( int j=0;j<=10;j++){
            if(j%2==0){
                System.out.print(j + " ");
            }
        }

	}

}
